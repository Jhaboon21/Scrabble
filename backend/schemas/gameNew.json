{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "handle",
        "player1",
        "player1Score",
        "player2",
        "player2Score"
    ],
    "properties": {
        "handle": {
            "title": "The handle Schema",
            "type": "string",
            "default": "",
            "examples": [
                "682db637"
            ]
        },
        "player1": {
            "title": "The player1 Schema",
            "type": "string",
            "default": "",
            "examples": [
                "jimbo"
            ]
        },
        "player1Score": {
            "title": "The player1Score Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                0
            ]
        },
        "player2": {
            "title": "The player2 Schema",
            "type": ["string", "null"],
            "default": "",
            "examples": [
                "otherguy123"
            ]
        },
        "player2Score": {
            "title": "The player2Score Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                0
            ]
        }
    },
    "examples": [{
        "handle": "682db637",
        "player1": "jimbo",
        "player1Score": 0,
        "player2": "otherguy123",
        "player2Score": 0
    }]
}